var parcelHelpers = require("@parcel/transformer-js/src/esmodule-helpers.js");
var _previewModelJs = require("../models/previewModel.js");
var _previewViewJs = require("../views/previewView.js");
var _previewViewJsDefault = parcelHelpers.interopDefault(_previewViewJs);
var _searchViewJs = require("../views/searchView.js");
var _searchViewJsDefault = parcelHelpers.interopDefault(_searchViewJs);
const controllerLoadPreview = async function() {
    try {
        const id = new URLSearchParams(location.search).get("id");
        if (!id) throw new Error("No results found :(");
        await _previewModelJs.loadPreview("6328911");
    } catch (error) {
        console.log(error);
    }
};
const controllerSearch = function() {
    const query = _searchViewJsDefault.default.getQuery();
    if (!query) return;
    window.location.replace(`/home.html/?search=${query}`);
};
const init = function() {
    _searchViewJsDefault.default.addHandleSearchClick(controllerSearch);
    _previewViewJsDefault.default.addHandleLoadPreview(controllerLoadPreview);
};
init();
